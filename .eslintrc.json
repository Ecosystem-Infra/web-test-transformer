{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 0,
        "valid-jsdoc": 0
    },
    "overrides": [
        {
            "files": ["test/*.js", "index.js"],
            "rules": {
                "max-len": 0
            }
        }
    ]
}
